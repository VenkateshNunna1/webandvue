package com.java.strings;

public class Ex2 {

	public static void main(String[] args) {
	String s="ram#123";
	isEx2(s);

	}
	static void isEx2(String s) {
		String s1="";
		for(int i=0;i<=s.length()-1;i++) {
			char c = s.toLowerCase().charAt(i);
			if(c>='a' && c<='z') {
				s1+=c;
			}
		}
		System.out.println(s1);
		
	}

}
